\nconst Signup = () => {\n  const [formState, setFormState] = useState({\n    username: \"\",\n    email: \"\",\n    password: \"\",\n  });\n  const [addUser, { error, data }] = useMutation(ADD_USER);\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    setFormState({\n      ...formState,\n      [name]: value,\n    });\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    console.log(formState);\n\n    try {\n      const { data } = await addUser({\n        variables: { ...formState },\n      });\n\n      Auth.login(data.addUser.token);\n    } catch (e) {\n      console.error(e);\n    }\n  };\n\n  return (\n    <Card sx={{ maxWidth: 345 }}>\n      <CardActionArea>\n        <CardMedia\n          component=\"img\"\n          height=\"140\"\n          image\n          src=\"../public/signup.jpg\"\n          alt=\"pug hating mondays\"\n        />\n        <CardContent>\n          <Typography gutterBottom variant=\"h5\" component=\"div\">\n            Sign up or Login\n          </Typography>\n          <Typography variant=\"body2\" color=\"text.secondary\">\n            Greetings! Welcome to Very Good Dogs Club where your dog get the\n            platform they need to write their thoughts about YOU.\n          </Typography>\n        </CardContent>\n      </CardActionArea>\n      <CardActions>\n        <Button size=\"small\" color=\"primary\">\n          {handleFormSubmit}Submit\n        </Button>\n      </CardActions>\n    </Card>\n  );\n};\nexport default Signup;\n",["74","75"],"/Users/nataliescott/Desktop/dogs-club/Very-good-dogs-club/client/src/utils/mutations.js",[],"/Users/nataliescott/Desktop/dogs-club/Very-good-dogs-club/client/src/utils/auth.js",[],"/Users/nataliescott/Desktop/dogs-club/Very-good-dogs-club/client/src/pages/Home.js",[],"/Users/nataliescott/Desktop/dogs-club/Very-good-dogs-club/client/src/pages/Profile.js",[],"/Users/nataliescott/Desktop/dogs-club/Very-good-dogs-club/client/src/